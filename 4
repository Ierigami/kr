#include <iostream>;
using namespace std;

int main() {

	setlocale(0, "");

	int n, curr; //количество вводимых значений, текущее значение

	cout << "Сколько значений будет введено?\n";
	cin >> n;

	int M1[9][4] = { 0 };

	for (int i = 0; i < n; i++) {
		cin >> curr;
		if (curr % 4 == 0) {
			M1[curr / 4][4] = 1;
		}
		else {
			M1[curr / 4 + 1][curr % 4] = 1;
		}
	}
	

	/*for (int i = 0; i < 9; i++) {
		for (int j = 0; j < 4; j++) {
			cout << M1[i][j] << ", ";
		}
		cout << '\n';
	}*/
}

